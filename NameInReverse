public class ReverseName {
    public static void main(String[] args) {
        String name = "KARTIKEYA";  // change this to your name
        
        // Reverse the name
        String reverse = "";
        for (int i = name.length() - 1; i >= 0; i--) {
            reverse += name.charAt(i);
        }

        System.out.println("Reversed name: " + reverse);
    }
}
